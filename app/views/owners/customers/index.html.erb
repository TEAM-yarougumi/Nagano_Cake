<table class="table table-hover table-inverse">
  <thead>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
   </thead>
  <% @customer.each do |customer| %>
    <tr>
      <td>
        <%=link_to customer.id,owners_edit_owner_costomer_path(customer)%>
      </td>
      <td>
        <%=link_to customer.last_name, owners_show_owner_costomer_path(customer) %>
      </td>
      <td>
        <%= customer.email %>
      </td>
      <td>
        <%= customer.status %>
      </td>
    </tr>
  <% end %>
</table>